public class Main {
	public static void main(String[] args) {
		int z=5;
        for(int i = z; i>= 1; i--){
            for(int j=i; j>=1; j--){
                System.out.print("* ");
            }
            System.out.println();
        for(int k=z; k>=i; k--){
            System.out.print(" ");
        }
        }


	}
}