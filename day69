public class Main {
    public static void main(String[] args) {
        int[][] nilai = {
            {1, 2, 3},
            {4, 5, 6},
            {7, 8, 9}
        };
        
        int total = 0;
        
        for (int i = 0; i < nilai.length; i++) {
            for (int j = 0; j < nilai[i].length; j++) {
                total += nilai[i][j];
            }
        }
        
        System.out.println("Jumlah total semua elemen: " + total);
    }
}

