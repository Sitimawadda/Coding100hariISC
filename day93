public class Main {
    
    public static void OperasiAritmatika(int a, int b){
        System.out.println(a + " + " + b + " = " + (a+b));
        System.out.println(a + " - " + b + " = " + (a-b));
        System.out.println(a + " x " + b + " = " + (a*b));
        System.out.println(a + " : " + b + " = " + (a/b));
        System.out.println(a + " % " + b + " = " + (a%b));
 
    }
    
    public static void main(String[] args) {
       OperasiAritmatika(60,20);
    }
    
}