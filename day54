import java.util.Arrays;
public class Main {
	public static void main(String[] args) {
		System.out.println("====menjumlahkan isi array dengan menggunakan for====");
		
		int[]nilai=new int[]{12,23,34,45,56};
		int jumlah=0;
		for(int i=0; i<nilai.length;i++){
			jumlah+=nilai[i];
		}
			System.out.println(jumlah);
		
		
	}		
}