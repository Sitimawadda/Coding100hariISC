public class Main {
	public static void main(String[] args) {
		int z= 5;
		int x=z/2;
		for(int i= 1; i<= z; i++){
			if(i <= x){
				for(int j=1;j <= i;j++){
					System.out.print("* ");
				}
			}else{
				for(int j=1; j<= z -i +1;j++){
					System.out.print("* ");
				}
			}
			System.out.println();
		}
				
	}
}