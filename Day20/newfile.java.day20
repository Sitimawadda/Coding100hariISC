public class Main {
	public static void main(String[] args) {
		int jumlah = 0;

        for (int i = 1; i <= 100; i++) {
            if (i % 3 == 0 || i % 5 == 0) {
                jumlah += i;
            }
        }

        System.out.println("Jumlah kelipatan 3 dan 5 antara 1 hingga 100 adalah: " + jumlah);
	}
}