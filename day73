public class ProgramAngkaGenap {
    public static void main(String[] args) {
        int[][] Angka = {
        	{1, 2, 3},
        	{4, 5, 6},
        	{7, 8, 9}
        	};

        System.out.println("Angka-angka dengan nilai genap:");
        for (int i = 0; i < Angka.length; i++) {
            for (int j = 0; j < Angka[i].length; j++) {
                if (Angka[i][j] % 2 == 0) {
                    System.out.print(Angka[i][j] + " ");
                }
            }
        }
    }
}